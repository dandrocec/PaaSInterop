    <!-- http://www.foi.unizg.hr/ontologies/2013/InteroperabilityProblemsOntology.owl#ApiOperationLevelProblem -->

    <owl:Class rdf:about="http://www.foi.unizg.hr/ontologies/2013/InteroperabilityProblemsOntology.owl#ApiOperationLevelProblem">
